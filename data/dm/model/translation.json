    "@attr": {
        "container": "multi",
        "type": "object"
    },
    "Author": {
        "@attr": {
            "container": "array",
            "type": "object"
        },
        "Name": {
            "@attr": {
                "default": "Unknown writer",
                "maxlen": 32,
                "type": "string"
            }
        }
    },
    "Label": {
        "@attr": {
            "default": "English",
            "maxlen": 32,
            "type": "string"
        }
    },
    "Steam": {
        "@attr": {
            "default": "english",
            "maxlen": 32,
            "type": "string"
        }
    },
    "Tokens": "@include translation/tokens.json"
}

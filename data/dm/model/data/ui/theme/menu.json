            }
        },
        "Blip": {
            "@attr": {
                "data": "Data.SFX",
                "default": "MenuBlip",
                "type": "link"
            }
        },
        "Pick": {
            "@attr": {
                "data": "Data.SFX",
                "default": "MenuPick",
                "type": "link"
            }
        }
    }
